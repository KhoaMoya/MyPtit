<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.khoa.scheduleview.ScheduleView
        android:id="@+id/schecule_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/colorBlack5"
        app:schedule_addableEvent="true"
        app:schedule_labelBackgroundColor="@color/colorWhite"
        app:schedule_labelTextColor="@color/colorBlack"
        app:schedule_labelTextSize="9sp"
        app:schedule_roomTextColor="@color/colorAccentDark"
        app:schedule_separateColor="#919191"
        app:schedule_subjectBackgroundColor="#fff"
        app:schedule_subjectTextColor="@color/colorBlack" />

</LinearLayout>