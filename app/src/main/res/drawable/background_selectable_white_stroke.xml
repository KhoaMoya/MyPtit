<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape >
            <stroke android:color="@color/colorGray"
                android:width="0.5dp"/>
        </shape>
    </item>

    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/colorBlack5"/>
        </shape>
    </item>
</selector>
